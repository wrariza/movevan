{
  "name": "movevan",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev:client": "ng serve",
    "dev:server": "nodemon",
    "build:client": "ng build",
    "test:client": "jest",
    "lint:client": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/server/api-module/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server/api-module/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig-server.json -c tslint-server.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config  src/server/test/jest-e2e.json",
    "webpack": "webpack --config webpack-server.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.3.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typescript": ">=2.7.2 <2.10.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./src/client/setup-jest.ts",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
